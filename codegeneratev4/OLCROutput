public static final String Preference_OLCRIpAddr = "OLCRIpAddr";
public String GetOLCRIpAddr() {;
return m_preferences.getString(Preference_OLCRIpAddr, "");
}
public void SetOLCRIpAddr(String strOLCRIpAddr) {
m_preferences.edit()
.putString(Preference_OLCRIpAddr, strOLCRIpAddr).commit();
}
